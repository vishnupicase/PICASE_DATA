	Where ([CASE_NUM] is null OR [CASE_NUM]='')

	    Update Stg.TPA
		Set IsValidRecord=0,
	ERROR_DESCRIPTION= isnull(ERROR_DESCRIPTION,'') + 'No Case data record'
		where CASE_NUM  not in(select CaseNumber from [dbo].[Cases]) 


END
