	[CARRIER_1_ADJUSTER_LAST_NAME] [nvarchar](max) NULL,
	[CARRIER_1_ADDRESS_1] [nvarchar](max) NULL,
	[CARRIER-_1_ADJUSTER_CITY] [nvarchar](max) NULL,
	[CARRIER_1_ADJUSTER_STATE] [nvarchar](max) NULL,
